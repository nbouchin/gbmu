#!/bin/sh

NAME=$(whoami)
PROJECT_PATH=$(pwd)
UNAME=$(uname -s)

if [ "$UNAME" == "Darwin" ]; then
	/sgoinfre/goinfre/Perso/$NAME/Qt/5.10.0/clang_64/bin/qmake $PROJECT_PATH/src/ui/gbmu.pro -spec macx-clang CONFIG+=debug CONFIG+=x86_64 CONFIG+=qml_debug
else
	qmake-qt5 $PROJECT_PATH/src/ui/gbmu.pro CONFIG+=debug CONFIG+=x86_64 CONFIG+=qml_debug
fi
